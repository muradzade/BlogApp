@model HomeSliderModel
@{
    ViewData["Title"] = "Index";
}


<div class="container">
    <div class="row">
        <div class="col-md-3">
            @await Component.InvokeAsync("CategoryMenu")
        </div>
        <div class="col-md-9">
            @Html.Partial("_Slider", Model.Slider)
            @Html.Partial("_BlogList", Model.Home)
        </div>
    </div>
</div>
